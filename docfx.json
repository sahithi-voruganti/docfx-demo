{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "src": "../../../3M/SourceCode/azure-iot-platform-dotnet/",
          "files":[
            "src/services/asa-manager/WebService/**.cs"
          ]
        }
      ],
      "dest": "api/asa-manager"
    },
    {
      "src": [
        {
          "src": "../../../3M/SourceCode/azure-iot-platform-dotnet/",
          "files":[
            "src/services/config/WebService/**.cs"
          ]
        }
      ],
      "dest": "api/config"
    }
  ],
  "build": {
    "content": [
      {
      "files": [
        "**/*.md",
        "**/*.yml"
      ]
      },
      {
        "src": "yml",
        "files": ["**/*.yml"],
        "dest": "web-ui"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "3M Serenity Documentation",
      "_appLogoPath": "images/3M_wordmark.svg",
      "_appFaviconPath": "images/3M_wordmark.svg",
      "_enableSearch": true,
      "_appFooter": "Copyright Â© 3M."
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}